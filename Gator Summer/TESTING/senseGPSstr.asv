function gpsData = senseGPSstr(COM)
    baudrate = 38400; %This is the defualt value of the GPS, the only way it can be modified is in u-center through a rewrite of the gps firmware
    gpsObj = serialport(COM,baudrate);
    parserObj = nmeaParser("MessageId","RMC");
    eval([COM ' = struct(''Lat'',[],''Lon'',[],''time'',[])']);  
    count=1;
     save(COM)
    while(1==1)
    % Read line by line from the serial object.
    data = readline(gpsObj);
    rmcData = parserObj(data);
    % Status = 0, indicates, the input NMEA sentence is an valid RMC
    % sentence.
    if rmcData.Status == 0
        eval([COM '.lat(count) = rmcData.Latitude']);
        eval([COM '.lon(count) = rmcData.Longitude']);
        eval([COM '.time(count) = rmcData.UTCDateTime']);
        datastored.lon(count) = rmcData.Longitude;
        datastored.time(count) = rmcData.UTCDateTime;
       datastored
        count=count+1;
    end
    end
end
